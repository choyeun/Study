filters:
  and:
    - file.tags.contains("군대")
    - file.tags.contains("사람")
views:
  - type: table
    name: 표
    order:
      - file.name
      - 계급
      - 전역일
      - 소속
      - 보직
      - 생활관
    sort:
      - property: 전역일
        direction: ASC
    columnSize:
      note.소속: 380
      note.보직: 164
